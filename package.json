{
  "name": "@robb_j/r0b-home",
  "private": true,
  "repository": "robb-j/www.r0b.io",
  "scripts": {
    "lint": "tsc --noEmit",
    "serve": "eleventy --serve",
    "format": "prettier --write '**/*.{js,json,css,md,ts,tsx}'",
    "build": "eleventy"
  },
  "dependencies": {
    "@openlab/alembic": "^0.2.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@types/11ty__eleventy-img": "^2.0.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.7",
    "lint-staged": "^13.0.3",
    "markdown-it-anchor": "^8.6.5",
    "prettier": "^2.4.1",
    "prism-themes": "^1.9.0",
    "typescript": "^4.8.3",
    "yorkie": "^2.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "gitHooks": {
    "pre-commit": "tsc && lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "prettier --write"
    ]
  }
}
